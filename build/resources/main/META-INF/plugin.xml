<idea-plugin>
  <id>com.rcvtestrunner.plugin</id>
  <name>RCV Test Runner</name>
  <vendor email="evgenii.truuts@ringcentral.com">Eugene Truuts</vendor>
  <version>1.4.4</version>
  <idea-version since-build="252" until-build="252.*" />
  <depends>com.intellij.modules.platform</depends>
  <extensions defaultExtensionNs="com.intellij">
    <toolWindow id="PW Runner" anchor="right" factoryClass="com.envrunner.EnvToolWindowFactory" />
    <notificationGroup id="PW Runner" displayType="BALLOON" isLogByDefault="true" />
  </extensions>
</idea-plugin>
